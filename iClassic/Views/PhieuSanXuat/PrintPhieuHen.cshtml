@{
    Layout = null;
}
@model IQueryable<iClassic.Models.Customer>
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script>
    $(function () {
        window.print();
    });
</script>
<h1 class="text-center">PHIẾU HẸN</h1>
<table class="table"> 
    <tr>
        <th>Tên Khách</th>
        <th>Số lượng</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.TenKH</td>
            <td>@item.SoDo</td>
        </tr>
    }    
</table>
